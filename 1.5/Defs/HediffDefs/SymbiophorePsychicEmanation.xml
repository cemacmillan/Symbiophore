<?xml version="1.0" encoding="utf-8" ?>
<Defs>
     <HediffDef>
         <defName>SymbiophorePsychicEmanation</defName>
         <label>symbiophore psychic emanations</label>
         <description>The Symbiophore emits a psychic field that affects nearby pawns.</description>
         <isBad>false</isBad>
         <hediffClass>HediffWithComps</hediffClass>
         <comps>
             <li Class="DIL_Symbiophore.HediffCompProperties_AnimalEmitter">
                 <range>20</range>
                 <thought>SymbiophoreMoodThought</thought>
                 <moodFactor>1.5</moodFactor>
             </li>
             <li Class="HediffCompProperties_DisappearsOnDeath"/>
         </comps>
     </HediffDef>
</Defs>